{
    "name": "yiannis/dokimes",
    "description": "My laravel test composer project",
    "keywords": ["yiannis", "dokimes"],
    "type": "project",
    "authors": [
        {
            "name": "Yiannis Christodoulou",
            "email": "yiannis@web357.com",
            "homepage": "http://www.yiannistaos.com/",
